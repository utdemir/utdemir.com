<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
<title>Utku Demir</title>
<description>Posts from Utku Demir's homepage</description>
<link>https://utdemir.com</link>
<atom:link href="https://utdemir.com/rss.xml" rel="self" type="application/rss+xml" />
<ttl>1800</ttl>
{% for item in null | get_activities() | sort(true, false, "date") %}
<item>
  <title>{{ item.title | safe }}</title>
  <link>{{ item.target_absolute }}</link>
  <guid>{{ item.target_absolute }}</guid>
  <pubDate>{{ item.rfc822_date }}</pubDate>
</item>
{% endfor %}
</channel>
</rss>